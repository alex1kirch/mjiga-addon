{
  "name": "azure-devops-addon",
  "version": "1.0.8",
  "description": "Miro integration for Azure DevOps and Team Foundation Server",
  "license": "MIT",
  "author": "miro",
  "scripts": {
    "prebuild:prod": "npm run clean",
    "build:prod": "tsc -p src/backend/tsconfig.build.json",
    "clean": "rimraf dist",
    "clean:client": "rimraf dist/public",
    "preclient:dev": "npm run clean:client",
    "client:dev": "webpack --mode=development --config webpack.app.js",
    "preclient:prod": "npm run clean:client",
    "client:prod": "webpack --mode=production --config webpack.app.js",
    "compile": "tsc --noEmit -p src/frontend && tsc --noEmit -p src/backend",
    "format": "prettier --write \"src/**/*.ts?(x)\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prestart": "npm run client:dev",
    "start": "ts-node --project src/backend/tsconfig.json -r tsconfig-paths/register src/backend/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:dev": "npm run client:dev",
    "start:dev": "nodemon",
    "start:prod": "node dist/src/backend/main.js",
    "prune:prod": "npm prune --production",
    "test": "jest",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:integration": "npm i",
    "test:integration": "ts-node ./src/backend/test/runner.ts",
    "test:watch": "jest --watch"
  },
  "jest": {
    "projects": [
      "<rootDir>/backend",
      "<rootDir>/frontend"
    ],
    "rootDir": "src"
  },
  "dependencies": {
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.6.7",
    "@nestjs/typeorm": "^6.1.3",
    "@sentry/node": "^5.6.2",
    "analytics-node": "^3.4.0-beta.1",
    "azure-devops-node-api": "^9.0.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "sqlite3": "^4.0.6",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "sharp": "^0.22.1",
    "typeorm": "^0.2.18",
    "typeorm-naming-strategies": "^1.1.0",
    "useragent": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.6.7",
    "@types/analytics-node": "^3.1.1",
    "@types/execa": "^2.0.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/node": "^10.14.17",
    "@types/node-fetch": "^2.5.0",
    "@types/passport": "^1.0.1",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/sharp": "^0.22.2",
    "@types/supertest": "^2.0.8",
    "@types/useragent": "^2.1.1",
    "@types/uuid": "^3.4.5",
    "bowser": "^2.6.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass-loader": "^7.3.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^0.23.1",
    "supertest": "^3.4.1",
    "svg-inline-loader": "^0.8.0",
    "svg-sprite-loader": "^4.1.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.5.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
