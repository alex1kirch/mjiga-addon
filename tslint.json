{
  "defaultSeverity": "error",
  "jsRules": {
    "no-unused-expression": true,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "no-console": false,
    "member-access": [false],
    "ordered-imports": [false],
    "member-ordering": [false],
    "interface-name": [false],
    "arrow-parens": false,
    "object-literal-sort-keys": false
  }
}
